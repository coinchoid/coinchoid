<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:id="@+id/announce_linear_layout">

	<include layout="@layout/deal_edit_layout" />

	<Button
	    android:id="@+id/button_go"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:layout_gravity="center"
	    android:text="@string/go" />

		    <View android:layout_width="fill_parent"
	android:layout_height="4dip" android:background="@color/white"></View>

	<TextView
	    android:id="@+id/distribution"
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:gravity="center"
	    android:text="distribution : Us_1"
	    android:paddingTop="10dip"
	    android:paddingBottom="10dip" />
	
	    <View android:layout_width="fill_parent"
	android:layout_height="4dip" android:background="@color/white" android:layout_marginBottom="4dip"></View>
	

	<net.homelinux.paubox.GameView
	    android:id="@+id/game_view"
	    android:layout_width="fill_parent"
	    android:layout_height="fill_parent" />

</LinearLayout>
